Algoritmo OperadoresRelacionales
	Definir a,b,c Como Entero
	a <- 10
	b <- 20
	c <- 30
	
	Escribir 
	Escribir (a+b >c)
	Escribir (a+b <c)
	Escribir (a+b =c)
	Escribir (a+b >=c)
	Escribir (a+b <=c)
	Escribir (a+b <>c)
	
	Escribir (a-b >c)
	Escribir (a-b <c)
	Escribir (a-b =c)
	Escribir (a-b >=c)
	Escribir (a-b <=c)
	Escribir (a-b <>c)
	
	Escribir (a*b >c)
	Escribir (a*b <c)
	Escribir (a*b =c)
	Escribir (a*b >=c)
	Escribir (a*b <=c)
	Escribir (a*b <>c)
	
	
FinAlgoritmo
